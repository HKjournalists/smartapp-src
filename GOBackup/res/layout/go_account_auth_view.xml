<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:background="@drawable/go_account_back" android:orientation="vertical"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:gravity="center_vertical|center_horizontal">
	<LinearLayout android:background="#000000"
		android:orientation="vertical" android:gravity="center_vertical|left"
		android:layout_width="wrap_content" android:layout_height="wrap_content">
		<TextView android:id="@+id/go_account_msg_title"
			android:text="@string/go_account_user_authority"
			android:layout_width="wrap_content" android:layout_height="wrap_content" />
		<ListView android:id="@+id/go_account_authority_listview"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:layout_marginTop="4dip" />
		<LinearLayout android:layout_height="wrap_content"
			android:layout_width="fill_parent" android:orientation="horizontal"
			android:gravity="center">
			<Button android:text="@string/go_account_ok" android:id="@+id/go_account_submit"
				android:layout_width="wrap_content" android:layout_height="wrap_content" />
			<Button android:id="@+id/go_account_permission_cancel"
				android:text="@string/go_account_cancel" android:layout_height="wrap_content"
				android:layout_width="wrap_content" />
		</LinearLayout>
	</LinearLayout>
</LinearLayout>