<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical" >

    <include layout="@layout/header_title_layout" />

    <RelativeLayout
        android:id="@+id/process_manager"
        android:layout_width="fill_parent"
        android:layout_height="75.0dip"
        android:background="@drawable/btn_selector_manage" >

        <ImageView
            android:id="@+id/abc"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dip"
            android:src="@drawable/icon_progress" />

        <TextView
            style="@style/app_manage_text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dip"
            android:layout_toRightOf="@id/abc"
            android:text="@string/progress_manager"
            android:textColor="#21a658" />

        <ImageView
            android:layout_width="16dp"
            android:layout_height="31dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="16dip"
            android:src="@drawable/jt_right" />
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="0.5dip"
        android:background="#EFEFEF" />

    <RelativeLayout
        android:id="@+id/cache_cleaner"
        android:layout_width="fill_parent"
        android:layout_height="75.0dip"
        android:background="@drawable/btn_selector_manage" >

        <ImageView
            android:id="@+id/abc"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dip"
            android:src="@drawable/v2_ic_cache_clear" />

        <TextView
            style="@style/app_manage_text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dip"
            android:layout_toRightOf="@id/abc"
            android:text="@string/cache_clear"
            android:textColor="#f2960c" />

        <ImageView
            android:layout_width="16dp"
            android:layout_height="31dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="16dip"
            android:src="@drawable/jt_right" />
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="0.5dip"
        android:background="#EFEFEF" />

    <RelativeLayout
        android:id="@+id/residualfiles"
        android:layout_width="fill_parent"
        android:layout_height="75.0dip"
        android:background="@drawable/btn_selector_manage" >

        <ImageView
            android:id="@+id/abc"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dip"
            android:src="@drawable/icon_rubbish" />

        <TextView
            style="@style/app_manage_text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dip"
            android:layout_toRightOf="@id/abc"
            android:text="@string/rubbish"
            android:textColor="#33b5e5" />

        <ImageView
            android:layout_width="16dp"
            android:layout_height="31dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="16dip"
            android:src="@drawable/jt_right" />
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="0.5dip"
        android:background="#EFEFEF" />

</LinearLayout>