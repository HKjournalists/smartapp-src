<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="appthem" parent="android:Theme.NoTitleBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/white</item>
    </style>

    <style name="tag_text">
        <item name="android:textColor">@color/grid_item_text</item>
        <item name="android:textSize">15sp</item>
        <item name="android:padding">5dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="left_progress">
        <item name="android:progressDrawable">@drawable/left_progress_green</item>
        <item name="android:background">@drawable/left_progres_gray</item>
    </style>

    <style name="left_menu_text">
        <item name="android:textColor">@color/left_menu_gray</item>
        <item name="android:textSize">@dimen/left_menu_text_size</item>
        <item name="android:layout_marginTop">8dp</item>
    </style>

    <style name="list_view_style" parent="@android:style/Widget.ListView">
        <item name="android:fadeScrollbars">true</item>
        <item name="android:headerDividersEnabled">false</item>
        <item name="android:scrollingCache">false</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:divider">@android:color/transparent</item>
        <item name="android:dividerHeight">0dp</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:fastScrollEnabled">false</item>
        <item name="android:footerDividersEnabled">false</item>
        <item name="android:drawSelectorOnTop">false</item>
        <item name="android:scrollbars">none</item>
        <item name="android:listSelector">@android:color/transparent</item>
    </style>

    <style name="gameName">
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="homegameName">
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="gameNameType">
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="home_tab_text">
        <item name="android:textColor">@color/home_tab_text</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="title_style">
        <item name="android:textColor">#777777</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="home_title">
        <item name="android:textColor">@color/left_menu_gray</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="home_icon_text">
        <item name="android:textColor">@color/left_menu_gray</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginTop">5dp</item>
    </style>

    <style name="catory_style">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="setting_text_style">
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="setting_check_style">
        <item name="android:button">@null</item>
        <item name="android:background">@drawable/set_check</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="my_gift_title">
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16.5sp</item>
        <item name="android:paddingLeft">@dimen/app_frampadding</item>
        <item name="android:paddingRight">@dimen/app_frampadding</item>
    </style>

    <style name="all_gift_title">
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="my_gift_date">
        <item name="android:textColor">@color/grid_black_text</item>
        <item name="android:textSize">13.5sp</item>
        <item name="android:paddingLeft">@dimen/app_frampadding</item>
        <item name="android:paddingRight">@dimen/app_frampadding</item>
    </style>

    <style name="radio_style">
        <item name="android:layout_height">40dp</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
        <item name="android:textColor">@color/radio_color</item>
        <item name="android:background">@drawable/serch_bg</item>
    </style>

    <style name="special_title">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="serch_layout">
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
    </style>

    <style name="white_serch_icon">
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:layout_marginRight">15dp</item>
    </style>

    <style name="app_edge">
        <item name="android:layout_marginLeft">@dimen/app_frampadding</item>
        <item name="android:layout_marginRight">@dimen/app_frampadding</item>
    </style>

    <style name="like_item">
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>

    <style name="goto_item_style">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">@drawable/get_gift_bg</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/get_gift_color</item>
    </style>

    <style name="all_gift_go" parent="goto_item_style">
        <item name="android:background">@drawable/get_gift_all_bg</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="gift_all_item_lost">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/left_menu_gray</item>
    </style>

    <style name="line_style">
        <item name="android:layout_height">1px</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:src">@color/line_gray</item>
    </style>

    <style name="gift_detail_title">
        <item name="android:textColor">@color/gitf_a</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:paddingLeft">@dimen/app_frampadding</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@color/tag_gray</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="gift_content">
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/left_menu_gray</item>
        <item name="android:paddingLeft">@dimen/app_frampadding</item>
    </style>

    <style name="gifit_detail_time">
        <item name="android:textColor">@color/gitf_a</item>
        <item name="android:textSize">15sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="game_desc">
        <item name="android:textColor">@color/left_menu_gray</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="limit_game_text">
        <item name="android:textColor">@color/left_menu_gray</item>
        <item name="android:textSize">16.5sp</item>
    </style>
    <style name="pro_layout">
        <item name="android:gravity">right</item>
    </style>
    <!-- 个人模块，小灰灰倾情奉献 0.0-->
    <!--
    <style name="person_tabview_text_unselect">
        <item name="android:textColor">@color/person_tabview_title_unselect</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="person_tabview_underline_select">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:background">@color/person_tabview_title_select</item>
    </style>
    -->
    <style name="my_game_more_layout">
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:layout_below">@id/gameIconLayout</item>
        <item name="android:background">@drawable/my_game_more_layout</item>
    </style>
    <style name="my_game_more_text">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_marginTop">5dp</item>
    </style>
    <style name="d_co_style">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:paddingBottom">3dp</item>
    </style>
    <style name="left_menu_cach">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/left_sdcard_gray</item>
    </style>

    <style name="left_not_read">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="them_detail">
        <item name="android:textColor">@color/gitf_a</item>
        <item name="android:textSize">16.5sp</item>
        <item name="android:lineSpacingExtra">5sp</item>
    </style>

    <style name="d_a_image_style">
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:background">@drawable/d_a_bg</item>
    </style>

    <style name="delete_style">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">#ff8800</item>
    </style>

    <style name="delete_cancel" parent="delete_style">
        <item name="android:textColor">@color/left_menu_gray</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">#fefefe</item>
    </style>
    <style name="game_detail_text">
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">13.5sp</item>
    </style>

    <style name="no_date_text">
        <item name="android:textColor">@color/left_menu_gray</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="comunity_co_vo_par">
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:textColor">@color/dymaic_amount_text_color</item>
        <item name="android:textSize">16sp</item>
        <item name="android:drawablePadding">6dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/press_bg</item>
    </style>

    <style name="Theme.loading" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>
</resources>