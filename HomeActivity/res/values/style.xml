<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="appthem" parent="android:Theme.NoTitleBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/white</item>
    </style>
    <style name="tag_text">
        <item name="android:textColor">@color/grid_item_text</item>
        <item name="android:textSize">15sp</item>
        <item name="android:padding">5dp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="left_progress">
        <item name="android:progressDrawable">@drawable/left_progress_green</item>
        <item name="android:background">@drawable/left_progres_gray</item>
    </style>
    <style name="left_menu_text">
        <item name="android:textColor">@color/left_menu_gray</item>
        <item name="android:textSize">@dimen/left_menu_text_size</item>
    </style>
    <style name="list_view_style" parent="@android:style/Widget.ListView">
        <item name="android:fadeScrollbars">true</item>
        <item name="android:headerDividersEnabled">false</item>
        <item name="android:scrollingCache">false</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:divider">@drawable/item_privider</item>
        <item name="android:dividerHeight">1dp</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:fastScrollEnabled">false</item>
        <item name="android:footerDividersEnabled">false</item>
        <item name="android:drawSelectorOnTop">false</item>
        <item name="android:scrollbars">none</item>
    </style>
    <style name="gameName">
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="homegameName">
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">16sp</item>
    </style>
    <style name="gameNameType">
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">13sp</item>
    </style>
    <style name="home_tab_text">
        <item name="android:textColor">@color/home_tab_text</item>
        <item name="android:textSize">13sp</item>
    </style>
    <style name="title_style">
        <item name="android:textColor">#777777</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="home_title">
        <item name="android:textColor">@color/left_menu_gray</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="home_icon_text">
        <item name="android:textColor">@color/left_menu_gray</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginTop">5dp</item>
    </style>
    <style name="catory_style">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="setting_text_style">
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">15sp</item>
    </style>
    <style name="setting_check_style">
        <item name="android:button">@null</item>
        <item name="android:background">@drawable/set_check</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="my_gift_title">
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="all_gift_title">
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18sp</item>
    </style>
    <style name="my_gift_date">
        <item name="android:textColor">@color/grid_black_text</item>
        <item name="android:textSize">16sp</item>
    </style>
    <style name="radio_style">
        <item name="android:layout_height">40dp</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
        <item name="android:textColor">@color/radio_color</item>
        <item name="android:background">@drawable/serch_bg</item>
    </style>
    <style name="special_title">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="serch_layout">
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
    </style>
    <style name="white_serch_icon">
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:layout_marginRight">15dp</item>
    </style>
    <style name="app_edge">
        <item name="android:layout_marginLeft">@dimen/app_frampadding</item>
        <item name="android:layout_marginRight">@dimen/app_frampadding</item>
    </style>
    <style name="like_item">
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>
    <style name="goto_item_style">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">@drawable/get_gift_bg</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/get_gift_color</item>
    </style>
    <style name="all_gift_go" parent="goto_item_style">
        <item name="android:background">@drawable/get_gift_all_bg</item>
    </style>
    <style name="gift_all_item_lost">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/left_menu_gray</item>
    </style>
    <style name="line_style">
        <item name="android:layout_height">1px</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:src">@color/line_gray</item>
    </style>
    <style name="gift_detail_title">
        <item name="android:textColor">@color/gitf_a</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:paddingLeft">@dimen/app_frampadding</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="gift_content">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/left_menu_gray</item>
        <item name="android:paddingLeft">@dimen/app_frampadding</item>
    </style>
    <style name="gifit_detail_time">
        <item name="android:textColor">@color/gitf_a</item>
        <item name="android:textSize">15sp</item>
    </style>
    <style name="game_desc">
        <item name="android:textColor">@color/left_menu_gray</item>
        <item name="android:textSize">15sp</item>
    </style>
    <style name="limit_game_text">
        <item name="android:textColor">@color/left_menu_gray</item>
        <item name="android:textSize">16.5sp</item>
    </style>
</resources>